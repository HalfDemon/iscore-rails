<% @usability_reports_submissions.each do |usability_reports_submission| %>
  <tr>
    <td><%= usability_reports_submission.site_id %></td>
    <td><%= usability_reports_submission.comments %></td>
    <td><%= usability_reports_submission.done_by %></td>
