<h1>Listing availability_service_checks</h1>

<table>
  <tr>
    <th>Service</th>
    <th>Availability check</th>
    <th>Site</th>
    <th>Available</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @availability_service_checks.each do |availability_service_check| %>
  <tr>
    <td><%= availability_service_check.service %></td>
    <td><%= availability_service_check.availability_check %></td>
    <td><%= availability_service_check.site %></td>
    <td><%= availability_service_check.available %></td>
    <td><%= link_to 'Show', availability_service_check %></td>
    <td><%= link_to 'Edit', edit_availability_service_check_path(availability_service_check) %></td>
    <td><%= link_to 'Destroy', availability_service_check, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

