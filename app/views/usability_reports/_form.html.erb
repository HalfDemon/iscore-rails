<%= simple_form_for(@usability_report) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :name %>

    <%= f.input :start_time %>

    <%= f.input :end_time %>

    <%= f.input :speical_instructions %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
